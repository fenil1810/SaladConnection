<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.saladconnection">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.USE_CREDENTIALS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/saladconnection"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ContactActivity"
            android:label="Contact Us" />
        <activity
            android:name=".FeedbackActivity"
            android:label="Feedback" />
        <activity
            android:name=".SoupActivity"
            android:label="Soups"
            android:parentActivityName=".MenuActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.android.saladconnection.MenuActivity" />
        </activity>
        <activity
            android:name=".SaladActivity"
            android:label="Salads" />
        <activity
            android:name=".MenuActivity"
            android:label="Menu" />
        <activity android:name=".SpicyPotatoDesc" />
        <activity android:name=".JuiceActivity" />
        <activity android:name=".OrderSummary" />
        <activity
            android:name=".SignInActivity"
            android:label="@string/title_activity_sign_in" />
        <activity android:name=".DbHelper" />
        <activity android:name=".SpinachSoup" />
        <activity android:name=".WatermelonBasildesc" />
        <activity android:name=".GoogleLoginActivity" />
        <activity android:name=".TestDB" />
        <activity android:name=".Soups2" />
        <activity android:name=".Salad2" />
        <activity android:name=".RetreivalActivity" />
        <activity android:name=".Juice2" />
        <activity android:name=".BarcodeActivity"></activity>
    </application>

</manifest>